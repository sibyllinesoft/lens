{
  "protection_rules": {
    "required_status_checks": {
      "strict": true,
      "contexts": [
        "ci/banned-patterns",
        "ci/hermetic-build",
        "ci/attestation-check"
      ]
    },
    "required_pull_request_reviews": {
      "required_approving_review_count": 2,
      "dismiss_stale_reviews": true
    },
    "restrictions": {
      "users": [
        "maintainer1",
        "maintainer2"
      ],
      "teams": [
        "security-team"
      ]
    }
  }
}