{
  "name": "lens",
  "version": "0.1.0",
  "description": "Local sharded code search system with three-layer processing pipeline",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "dev": "tsx watch src/server.ts",
    "start": "node dist/server.js",
    "test": "vitest",
    "test:coverage": "vitest --coverage",
    "test:ui": "vitest --ui",
    "lint": "eslint src/**/*.ts",
    "fmt": "prettier --write src/**/*.ts",
    "validate:config": "cue eval architecture.cue",
    "validate:production": "cue export architecture.cue --expression lens_production"
  },
  "keywords": [
    "code-search",
    "indexing",
    "AST",
    "semantic-search",
    "performance"
  ],
  "author": "Lens Search",
  "license": "MIT",
  "dependencies": {
    "@fastify/cors": "^9.0.1",
    "@opentelemetry/api": "^1.7.0",
    "@opentelemetry/auto-instrumentations-node": "^0.41.0",
    "@opentelemetry/exporter-jaeger": "^1.17.2",
    "@opentelemetry/exporter-prometheus": "^0.45.0",
    "@opentelemetry/instrumentation": "^0.45.1",
    "@opentelemetry/sdk-node": "^0.45.1",
    "fast-fuzzy": "^1.12.0",
    "fastify": "^4.24.3",
    "nats": "^2.19.0",
    "pino": "^8.17.2",
    "uuid": "^9.0.1",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/node": "^20.10.4",
    "@types/uuid": "^9.0.7",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "@vitest/coverage-v8": "^1.6.1",
    "@vitest/ui": "^1.6.1",
    "eslint": "^8.55.0",
    "prettier": "^3.1.1",
    "tsx": "^4.6.2",
    "typescript": "^5.9.2",
    "vitest": "^1.6.1"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
